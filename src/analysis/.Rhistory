ROOTPATH <- substr(getwd(), 1, nchar(getwd())-12)
ROOTPATH
#load(paste(ROOTPATH, "data/yt_resplevel_data.Rdata", sep=""))
#write.csv(yt_resplevel, paste(ROOTPATH, "data/yt_resplevel_data.csv", sep=""))
reg_table <- read.csv(paste(ROOTPATH, "output/reg_table_filter_ytb_activities_122324.csv", sep=""))
reg_table$age = reg_table$age / 10
reg_table$timespan_imputed = reg_table$timespan_imputed / 60
#reg_table$extreme_lftRght = abs(reg_table$lftRght - 4)
#reg_table$extreme_antiPro = abs(reg_table$feel_thm_fdsz - 5)
#reg_table$extreme_lftRght[is.na(reg_table$extreme_lftRght)] <- 0
#reg_table$extreme_antiPro[is.na(reg_table$extreme_antiPro)] <- 0
reg_table
#load(paste(ROOTPATH, "data/yt_resplevel_data.Rdata", sep=""))
#write.csv(yt_resplevel, paste(ROOTPATH, "data/yt_resplevel_data.csv", sep=""))
reg_table <- read.csv(paste(ROOTPATH, "output/reg_table_filter_ytb_activities_122324.csv", sep=""))
reg_table$age = reg_table$age / 10
reg_table$timespan_imputed = reg_table$timespan_imputed / 60
#reg_table$extreme_lftRght = abs(reg_table$lftRght - 4)
#reg_table$extreme_antiPro = abs(reg_table$feel_thm_fdsz - 5)
#reg_table$extreme_lftRght[is.na(reg_table$extreme_lftRght)] <- 0
#reg_table$extreme_antiPro[is.na(reg_table$extreme_antiPro)] <- 0
reg_table
library(glmmTMB)
nbr.view <- glmmTMB(
view_counts ~ gender + age + education + timespan_imputed + interest_politics + if_left + if_right,
#view_counts ~ gender + age + education + timespan_imputed + interest_politics + extreme_lftRght,
weights = weight,
data = subset(reg_table, if_view==1),
family = nbinom2)
nbr.subscribe <- glmmTMB(
subscribe_counts ~ gender + age + education + timespan_imputed + interest_politics + if_left + if_right,
#subscribe_counts ~ gender + age + education + timespan_imputed + interest_politics + extreme_lftRght,
weights = weight,
#data = subset(reg_table, if_subscribe==1),
data = reg_table,
family = nbinom2)
nbr.comment <- glmmTMB(
comment_counts ~ gender + age + education + timespan_imputed + interest_politics + if_left + if_right,
#comment_counts ~ gender + age + education + timespan_imputed + interest_politics + extreme_lftRght,
weights = weight,
#data = subset(reg_table, if_comment==1),
data = reg_table,
family = nbinom2)
library(dotwhisker)
models <- list(comment=nbr.comment, subscribe=nbr.subscribe, view=nbr.view)
reg_plot = dwplot(models,
dot_args=list(size=5),
whisker_args=list(size=1.5)) +
facet_grid(~model, scales="free_x") +
theme_bw(base_size = 10) +
geom_vline(xintercept = 0, colour = "black", linetype = 2, size=1) +
scale_color_manual(values=c("#4582b4", "#ff4500","#2d8b57")) +
theme(legend.position="none") +
#ggtitle("(B)") +
theme(
plot.title = element_text(face = "bold", size=24),
text = element_text(size = 22),
axis.text.x = element_text(size=15)) +
scale_y_discrete(labels = c("right", "left", "interest in politics", "timespan", "education", "age","gender"))
#scale_y_discrete(labels = c("left/right\nextremity", "interest in politics", "timespan", "education", "age","gender"))
#scale_y_discrete(labels = c("anti-gov", "pro-gov", "interest in politics", "timespan", "education", "age","gender"))
#scale_y_discrete(labels = c("anti-/pro-gov\nextremity", "interest in politics", "timespan", "education", "age","gender"))
ggsave(
paste(ROOTPATH, "output/results-summary-122724/alt-sample/nbr_coeff_nbinom2_lftrght_alt_sample.pdf", sep=""),
device="pdf",
width=10,
plot=reg_plot)
reg_plot
